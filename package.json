{
  "name": "thinkwp",
  "version": "1.0.0",
  "description": "thinkwp",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^2.1.1",
    "dotenv": "^8.2.0",
    "find-config": "^1.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "node-sass-glob-importer": "^5.3.1",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^4.0.0",
    "postcss-nested": "^5.0.6",
    "sass-loader": "^7.1.0",
    "style-dictionary": "^3.0.2",
    "tailwindcss": "^2.2.16",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "start": "webpack --watch",
    "start:dev": "webpack --watch --mode development",
    "tokens": "style-dictionary build --config style-dictionary.config.json",
    "prettier": "prettier --write sass/**/*.scss js/**/*.js",
    "tw.build": "NODE_ENV=production tailwindcss -i ./sass/tailwind.css -o ./dist/tailwind.css --minify",
    "tw.dev": "NODE_ENV=development tailwindcss -i ./sass/tailwind.css -o ./dist/tailwind.css --watch"
  },
  "author": "ThinkShout, Inc",
  "license": "GPL-3.0",
  "dependencies": {
    "aria-autocomplete": "^1.4.0",
    "copy-to-clipboard": "^3.3.1",
    "custom-select": "^1.1.15",
    "penpal": "^5.3.0",
    "prettier-plugin-twig-melody": "^0.4.6"
  }
}
